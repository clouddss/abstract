{
  "name": "abstract-pump-backend",
  "version": "1.0.0",
  "description": "Backend indexer and API for Abstract Pump Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "start:prod": "node dist/index.js",
    "indexer": "ts-node-dev --respawn --transpile-only src/indexer/index.ts",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "db:seed": "ts-node src/database/seed.ts",
    "test:seed": "tsx src/scripts/seed-test-data.ts",
    "db:reset": "npx prisma migrate reset --force",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "defi",
    "indexer",
    "api",
    "abstract",
    "ethereum"
  ],
  "author": "Abstract Pump Platform",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "ethers": "^6.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "ws": "^8.14.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.3.0"
  }
}
